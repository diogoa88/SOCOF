package pl5;

public class main {

	public static void main(String[] args) {

		LockFreeQueue<Integer> queue = new LockFreeQueue<Integer>();

		for (int i = 0; i < 10; i++) {
			new Aux(i, queue).start();
		}

		queue.print();
		for (int j = 0; j < 7; j++) {
			new Thread() {
				@Override
				public void run() {
					queue.pop();
				}
			}.start();
		}
		
		Thread.sleep(1000);
		queue.print();

	}

}
